tpadding: 0;\n\t\t\t&.underline {\n\t\t\t\t.header-underline();\n\t\t\t}\n\t\t\t&.padded {\n\t\t\t\tpadding: 0.5em 0 0.2em 0;\n\t\t\t}\n\t\t}\n\t\t&-normal {\n\t\t\tfont-size: @type-size-normal;\n\t\t\tline-height: @type-line-height;\n\t\t\tpadding: 0;\n\t\t\t&.padded {\n\t\t\t\tpadding: 0.5em 0 0.2em 0;\n\t\t\t}\n\t\t}\n\t\t&-small {\n\t\t\tfont-size: @type-size-small;\n\t\t\tline-height: @type-line-height;\n\t\t\tpadding: 0;\n\t\t\t&.padded {\n\t\t\t\tpadding: 0.5em 0 0.2em 0;\n\t\t\t}\n\t\t}\n\t}\n\n\t&-weight {\n\t\t&-ultralight {\n\t\t\t#font-weight > .ultralight();\n\t\t}\n\t\t&-light, &-thin {\n\t\t\t#font-weight > .light();\n\t\t}\n\t\t&-normal {\n\t\t\t#font-weight > .normal();\n\t\t}\n\t\t&-semibold, &-medium {\n\t\t\t#font-weight > .semibold();\n\t\t}\n\t\t&-bold {\n\t\t\t#font-weight > .bold();\n\t\t}\n\t\t&-black {\n\t\t\t#font-weight > .black();\n\t\t}\n\t}\n\n\t&-style {\n\t\t&-shadow {\n\t\t\t#font-style > .shadow();\n\t\t}\n\t\t&-emboss {\n\t\t\t#font-style > .emboss();\n\t\t}\n\t}\n}\n\n.header-underline() {\n\tborder-bottom: 1px solid @color-mid-light-grey;\n}\n","//********************************************************\n// STANDALONE COLOR CLASSES\n//\n// Copy color variables from variable-colors.less\n// Regex Find: (@color-([\\w|-])*):(.*)\\n\n// Replace: #color > .generate(\"$2\", $1);\\n\n\n\n#color {\n\t.generate(@name, @hex) {\n\t\t.color-border-@{name} {\n\t\t\tborder-color: @hex !important;\n\t\t}\n\t\t.color-background-@{name} {\n\t\t\tbackground-color: @hex !important;\n\t\t}\n\t\t.color-text-@{name} {\n\t\t\tcolor: @hex !important;\n\t\t}\n\t}\n}\n\n\n//********************************************************\n// GREY\n\n#color > .generate(~\"black\", @color-black);\n#color > .generate(~\"darkest-grey\", @color-darkest-grey);\n#color > .generate(~\"dark-grey\", @color-dark-grey);\n#color > .generate(~\"mid-dark-grey\", @color-mid-dark-grey);\n#color > .generate(~\"mid-grey\", @color-mid-grey);\n#color > .generate(~\"mid-light-grey\", @color-mid-light-grey);\n#color > .generate(~\"light-grey\", @color-light-grey);\n#color > .generate(~\"lighter-grey\", @color-lighter-grey);\n#color > .generate(~\"lightest-grey\", @color-lightest-grey);\n#color > .generate(~\"white\", @color-white);\n\n\n//********************************************************\n// BLUE\n\n#color > .generate(~\"dark-blue\", @color-dark-blue);\n#color > .generate(~\"blue\", @color-blue);\n#color > .generate(~\"light-blue\", @color-light-blue);\n#color > .generate(~\"lighter-blue\", @color-lighter-blue);\n#color > .generate(~\"lightest-blue\", @color-lightest-blue);\n\n\n//********************************************************\n// ORANGE\n\n#color > .generate(~\"orange\", @color-orange);\n#color > .generate(~\"brand-orange\", @color-brand-orange);\n#color > .generate(~\"light-orange\", @color-light-orange);\n#color > .generate(~\"lightest-orange\", @color-lightest-orange);\n\n\n//********************************************************\n// YELLOW\n\n#color > .generate(~\"dark-yellow\", @color-dark-yellow);\n#color > .generate(~\"yellow\", @color-yellow);\n#color > .generate(~\"light-yellow\", @color-light-yellow);\n\n\n//********************************************************\n// RED\n\n#color > .generate(~\"darker-red\", @color-darker-red);\n#color > .generate(~\"dark-red\", @color-dark-red);\n#color > .generate(~\"red\", @color-red);\n#color > .generate(~\"light-red\", @color-light-red);\n#color > .generate(~\"lightest-red\", @color-lightest-red);\n\n\n//********************************************************\n// GREEN\n\n#color > .generate(~\"green\", @color-green);\n#color > .generate(~\"light-green\", @color-light-green);\n#color > .generate(~\"lightest-green\", @color-lightest-green);\n","//********************************************************\n// IMAGES\n\nimg {\n\tdisplay: inline-block;\n\t&.block {\n\t\tdisplay: block;\n\t}\n}\n\na.img {\n\tdisplay: inline-block;\n\timg {\n\t\tdisplay: block;\n\t}\n}\n","//********************************************************\n// MEDIA\n\nobject, embed {\n\toutline: 0;\n}\n","//********************************************************\n// ALIGNMENT\n\n\n//********************************************************\n// PADDING & MARGIN\n\n// Padding\n.no-hpadding {\n\tpadding-left: 0 !important;\n\tpadding-right: 0 !important;\n}\n\n.no-vpadding {\n\tpadding-top: 0 !important;\n\tpadding-bottom: 0 !important;\n}\n\n.no-padding {\n\tpadding: 0 !important;\n}\n\n// Margin\n.no-hmargin {\n\tmargin-left: 0 !important;\n\tmargin-right: 0 !important;\n}\n\n.no-vmargin {\n\tmargin-top: 0 !important;\n\tmargin-bottom: 0 !important;\n}\n\n.no-margin {\n\tmargin: 0 !important;\n}\n\n// Margin & Padding\n.no-hspace {\n\tmargin-left: 0 !important;\n\tmargin-right: 0 !important;\n\tpadding-left: 0 !important;\n\tpadding-right: 0 !important;\n}\n\n.no-vmargin {\n\tmargin-top: 0 !important;\n\tmargin-bottom: 0 !important;\n\tpadding-top: 0 !important;\n\tpadding-bottom: 0 !important;\n}\n\n.no-margin {\n\tmargin: 0 !important;\n\tpadding: 0 !important;\n}\n\n\n//********************************************************\n// FLOATING ALIGNMENT\n\n.align-with {\n\t&-biggest {\n\t\tmargin-top: (@type-size-biggest / 2) + 6px !important;\n\t}\n\t&-bigger {\n\t\tmargin-top: (@type-size-bigger / 2) + 4px !important;\n\t}\n\t&-big {\n\t\tmargin-top: (@type-size-big / 2) + 2px !important;\n\t}\n}\n","//********************************************************\n// SPACING HELPERS\n\n#helpers {\n\t.all(@px) {\n\t\t.margin(@px);\n\t\t.margin-em(@px);\n\t\t.padding(@px);\n\t\t.padding-em(@px);\n\n\t\t.margin(@px) {\n\t\t\t.m-@{px}        { margin: ~\"@{px}px\"; }\n\t\t\t.m-top-@{px}    { margin-top: ~\"@{px}px\"; }\n\t\t\t.m-right-@{px}  { margin-right: ~\"@{px}px\"; }\n\t\t\t.m-bottom-@{px} { margin-bottom: ~\"@{px}px\"; }\n\t\t\t.m-left-@{px}   { margin-left: ~\"@{px}px\"; }\n\t\t\t.m-vert-@{px}   { margin-top: ~\"@{px}px\"; margin-bottom: ~\"@{px}px\"; }\n\t\t\t.m-horiz-@{px}  { margin-left: ~\"@{px}px\"; margin-right: ~\"@{px}px\"; }\n\t\t}\n\n\t\t.margin-em(@px) {\n\t\t\t@em: @px/10;\n\t\t\t// @em_name: ~`\"@{em}\".replace(/\\./g, '')`;\n\n\t\t\t.m-@{px}em        { margin: ~\"@{em}em\"; }\n\t\t\t.m-top-@{px}em    { margin-top: ~\"@{em}em\"; }\n\t\t\t.m-right-@{px}em  { margin-right: ~\"@{em}em\"; }\n\t\t\t.m-bottom-@{px}em { margin-bottom: ~\"@{em}em\"; }\n\t\t\t.m-left-@{px}em   { margin-left: ~\"@{em}em\"; }\n\t\t\t.m-vert-@{px}em   { margin-top: ~\"@{em}em\"; margin-bottom: ~\"@{em}em\"; }\n\t\t\t.m-horiz-@{px}em  { margin-left: ~\"@{em}em\"; margin-right: ~\"@{em}em\"; }\n\t\t}\n\n\t\t.padding(@px) {\n\t\t\t.p-@{px}        { padding: ~\"@{px}px\"; }\n\t\t\t.p-top-@{px}    { padding-top: ~\"@{px}px\"; }\n\t\t\t.p-right-@{px}  { padding-right: ~\"@{px}px\"; }\n\t\t\t.p-bottom-@{px} { padding-bottom: ~\"@{px}px\"; }\n\t\t\t.p-left-@{px}   { padding-left: ~\"@{px}px\"; }\n\t\t\t.p-vert-@{px}   { padding-top: ~\"@{px}px\"; padding-bottom: ~\"@{px}px\"; }\n\t\t\t.p-horiz-@{px}  { padding-left: ~\"@{px}px\"; padding-right: ~\"@{px}px\"; }\n\t\t}\n\n\t\t.padding-em(@px) {\n\t\t\t@em: @px/10;\n\t\t\t// @em_name: ~`\"@{em}\".replace(/\\./g, '')`;\n\n\t\t\t.p-@{px}em        { padding: ~\"@{em}em\"; }\n\t\t\t.p-top-@{px}em    { padding-top: ~\"@{em}em\"; }\n\t\t\t.p-right-@{px}em  { padding-right: ~\"@{em}em\"; }\n\t\t\t.p-bottom-@{px}em { padding-bottom: ~\"@{em}em\"; }\n\t\t\t.p-left-@{px}em   { padding-left: ~\"@{em}em\"; }\n\t\t\t.p-vert-@{px}em   { padding-top: ~\"@{em}em\"; padding-bottom: ~\"@{em}em\"; }\n\t\t\t.p-horiz-@{px}em  { padding-left: ~\"@{em}em\"; padding-right: ~\"@{em}em\"; }\n\t\t}\n\t}\n}\n\n#helpers > .all(0);\n#helpers > .all(5);\n#helpers > .all(10);\n#helpers > .all(15);\n#helpers > .all(20);\n#helpers > .all(30);\n#helpers > .all(40);\n","//********************************************************\n// TYPE\n\np {\n\tmargin: 0;\n\tpadding: 0.5em 0;\n}\n\nsmall {\n\tfont-size: @type-size-small;\n}\n\nstrong {\n\tfont-weight: bold;\n}\n\nem {\n\tfont-style: italic;\n}\n\ncite {\n\tfont-style: normal;\n}\n\n\n//********************************************************\n// UTILITY CLASSES\n\n// @todo\n.muted               { color: @color-mid-grey !important; }\n\na.muted:focus,\na.muted:hover        { color: darken(@color-mid-grey, 10%) !important; }\n\n.text-warning        { color: @color-text-warning !important; }\n\na.text-warning:focus,\na.text-warning:hover { color: darken(@color-text-warning, 10%) !important; }\n\n.text-error          { color: @color-text-error !important; }\n\na.text-error:focus,\na.text-error:hover   { color: darken(@color-text-error, 10%) !important; }\n\n.text-info           { color: @color-text-info !important; }\n\na.text-info:focus,\na.text-info:hover    { color: darken(@color-text-info, 10%) !important; }\n\n.text-success        { color: @color-text-success !important; }\n\na.text-success:focus,\na.text-success:hover { color: darken(@color-text-success, 10%) !important; }\n\n.text-left           { text-align: left !important; }\n.text-right          { text-align: right !important; }\n.text-center         { text-align: center !important; }\n\n\n//********************************************************\n// LISTS\n\n// Unordered and Ordered lists\nul, ol {\n\tpadding: 0;\n\tmargin: 0 0 @type-line-height / 2 25px;\n}\n\nul ul,\nul ol,\nol ol,\nol ul {\n\tmargin-bottom: 0;\n}\n\nli {\n\tline-height: @type-line-height;\n}\n\nli {\n\th1, h2, h3, h4, h5 {\n\t\tpadding-bottom: 0;\n\t}\n}\n\n// Remove default list styles\nul.unstyled,\nol.unstyled {\n\tmargin-left: 0;\n\tlist-style: none;\n\n\t> li {\n\t\tdisplay: block;\n\t\tpadding-left: 0;\n\t\tmargin-left: 0;\n\t}\n}\n\n// Single-line list items\nul.inline,\nol.inline {\n\tmargin-left: 0;\n\tlist-style: none;\n\n\t> li {\n\t\tdisplay: inline-block;\n\t\tpadding-left: 5px;\n\t\tpadding-right: 5px;\n\t}\n}\n\n// Description Lists\ndl {\n\tmargin-bottom: @type-line-height;\n}\n\ndt,\ndd {\n\tline-height: @type-line-height;\n}\n\ndt {\n\tfont-weight: bold;\n}\n\ndd {\n\tmargin-left: @type-line-height / 2;\n}\n\n// Horizontal layout (like forms)\n.dl-horizontal {\n\t.clearfix(); // Ensure dl clears floats if empty dd elements present\n\n\tdt {\n\t\tfloat: left;\n\t\twidth: @horizontal-component-offset - 20;\n\t\tclear: left;\n\t\ttext-align: right;\n\t\t//.text-overflow();\n\t}\n\n\tdd {\n\t\tmargin-left: @horizontal-component-offset;\n\t}\n}\n\n\n//********************************************************\n// MISCELLANEOUS\n\n// Horizontal rules\nhr {\n\tmargin: @type-line-height 0;\n\tborder: 0;\n\tborder-top: 1px dotted @hr-border;\n\tborder-bottom: 1px dotted @color-white;\n\tspeak: none;\n}\n\n// Abbreviations and acronyms\nabbr[title],\n// Added data-* attribute to help out our tooltip plugin, per https://github.com/twitter/bootstrap/issues/5257\nabbr[data-original-title] {\n\tcursor: help;\n\tborder-bottom: 1px dotted @color-light-grey;\n}\nabbr.initialism {\n\tfont-size: 90%;\n\ttext-transform: uppercase;\n}\n\n// Blockquotes\nblockquote {\n\tpadding: 0 0 0 15px;\n\tmargin: @type-line-height 0 @type-line-height 20px;\n\tborder-left: 5px solid @color-lighter-grey;\n\n\t&.indent1 { margin-left: 20px + (40px * 1); }\n\t&.indent2 { margin-left: 20px + (40px * 2); }\n\t&.indent3 { margin-left: 20px + (40px * 3); }\n\n\tp {\n\t\tmargin-bottom: 0;\n\t\t#font > .shorthand(@type-size-big, 300, @type-line-height * 1.25);\n\n\t\tcode {\n\t\t\tfont-size: 0.9em;\n\t\t\tline-height: inherit;\n\t\t}\n\t}\n\n\tsmall {\n\t\tdisplay: block;\n\t\tline-height: @type-line-height;\n\t\tcolor: @color-mid-grey;\n\n\t\t&:before {\n\t\t\tcontent: '\\2014 \\00A0';\n\t\t}\n\t}\n\n\t// Float right with text-align: right\n\t&.pull-right {\n\t\tfloat: right;\n\t\tpadding-right: 15px;\n\t\tpadding-left: 0;\n\t\tborder-right: 5px solid @color-lighter-grey;\n\t\tborder-left: 0;\n\n\t\tp, small {\n\t\t\ttext-align: right;\n\t\t}\n\n\t\tsmall {\n\t\t\t&:before {\n\t\t\t\tcontent: '';\n\t\t\t}\n\n\t\t\t&:after {\n\t\t\t\tcontent: '\\00A0 \\2014';\n\t\t\t}\n\t\t}\n\t}\n}\n\n// Quotes\nblockquote:before,\nblockquote:after {\n\tcontent: \"\";\n}\n\nq:before {\n\tcontent: \"\\201C\";\n}\n\nq:after {\n\tcontent: \"\\201D\";\n}\n","//********************************************************\n// HEADINGS\n\nh1, h2, h3, h4, h5, h6 {\n\tmargin: 0;\n\tpadding: 0.5em 0 0.2em 0;\n\tfont-family: @typeface-sans;\n\tfont-weight: normal;\n\tcolor: @color-text-normal;\n\ttext-rendering: optimizeLegibility; // Fix the character spacing for headings\n\n\tsmall {\n\t\tfont-weight: normal;\n\t\tline-height: 1;\n\t\tcolor: @color-mid-grey;\n\t}\n\n\t&.underline {\n\t\tborder-bottom: 1px solid @color-mid-light-grey;\n\t}\n}\n\nh1 {\n\tfont-size: @type-size-biggest;\n\tline-height: @type-line-height-biggest;\n\tsmall {\n\t\tfont-size: @type-size-bigger;\n\t}\n}\n\nh2 {\n\tfont-size: @type-size-bigger;\n\tline-height: @type-line-height-bigger;\n\tsmall {\n\t\tfont-size: @type-size-big;\n\t}\n}\n\nh3 {\n\tfont-size: @type-size-big;\n\tline-height: @type-line-height-big;\n\tsmall {\n\t\tfont-size: @type-size-normal;\n\t}\n}\n\nh4 {\n\tfont-size: @type-size-normal;\n\tline-height: @type-line-height;\n\tsmall {\n\t\tfont-size: @type-size-small;\n\t}\n}\n\nh5 {\n\tfont-size: @type-size-small;\n\tline-height: @type-line-height;\n}\n\n.h-underline {\n\t.header-underline();\n}\n\n\n//********************************************************\n// PAGE HEADER\n\n.page-header {\n\tpadding-bottom: (@type-line-height / 2) - 1;\n\tmargin: @type-line-height 0 (@type-line-height * 1.5);\n\tborder-bottom: 1px solid @color-light-grey;\n}\n","//********************************************************\n// FORMS\n\n// Make all forms have space below them\nform {\n\tmargin: 0 0 @type-line-height 0;\n}\n\nfieldset {\n\tpadding: 1em 2em;\n\tmargin: 1em;\n\tborder: 1px solid #e5e5e5;\n}\n\n// Groups of fields with labels on top (legends)\nlegend {\n\tdisplay: inline-block;\n\tpadding: 0 1em;\n\tmargin: 0;\n\tfont-size: @type-size-big;\n\tline-height: @type-line-height;\n\tcolor: @color-dark-grey;\n\t.border-radius(5px);\n\n\t// Small\n\tsmall {\n\t\tfont-size: @type-size-small;\n\t\tcolor: @color-mid-grey;\n\t}\n}\n\n// Set font for forms\nlabel,\ninput,\nbutton,\nselect,\ntextarea {\n\t#font > .shorthand(@type-size-normal, normal, @type-line-height);\n}\n\ninput,\nbutton,\nselect,\ntextarea {\n\tfont-family: @typeface-base;\n}\n\n// Identify controls by their labels\nlabel {\n\tdisplay: block;\n\tmargin-bottom: 5px;\n}\n\n\n//********************************************************\n// STANDARD CONTROLS\n\n// Shared size and type resets\nselect,\ntextarea,\ninput[type=\"text\"],\ninput[type=\"password\"],\ninput[type=\"datetime\"],\ninput[type=\"datetime-local\"],\ninput[type=\"date\"],\ninput[type=\"month\"],\ninput[type=\"time\"],\ninput[type=\"week\"],\ninput[type=\"number\"],\ninput[type=\"email\"],\ninput[type=\"url\"],\ninput[type=\"search\"],\ninput[type=\"tel\"],\ninput[type=\"color\"],\n.uneditable-input {\n\tdisplay: inline-block;\n\t//height: @type-line-height;\n\tpadding: 4px 6px;\n\tmargin-bottom: @type-line-height / 2;\n\tfont-size: @type-size-normal;\n\tline-height: 1.5em;\n\tcolor: @color-mid-dark-grey;\n\t.border-radius(@form-input-border-radius);\n\tvertical-align: middle;\n\t.box-sizing(border-box);\n}\n\n// Reset appearance properties for textual inputs and textarea\n// Declare width for legacy (can't be on input[type=*] selectors or it's too specific)\ninput,\n.uneditable-input {\n\twidth: 206px; // plus 12px padding and 2px border\n}\n\n// Reset height since textareas have rows\ntextarea {\n\twidth: 100%;\n\theight: auto;\n}\n\n// Everything else\ntextarea,\ninput[type=\"text\"],\ninput[type=\"password\"],\ninput[type=\"datetime\"],\ninput[type=\"datetime-local\"],\ninput[type=\"date\"],\ninput[type=\"month\"],\ninput[type=\"time\"],\ninput[type=\"week\"],\ninput[type=\"number\"],\ninput[type=\"email\"],\ninput[type=\"url\"],\ninput[type=\"search\"],\ninput[type=\"tel\"],\ninput[type=\"color\"],\n.uneditable-input {\n\tbackground-color: @form-input-background;\n\tborder: 1px solid @form-input-border;\n\t.box-shadow(inset 0 1px 3px rgba(0, 0, 0, .1));\n\t.transition(~\"border linear .2s, box-shadow linear .2s, background-color linear .2s\");\n\n\t// Focus state\n\t&:focus {\n\t\tbackground-color: @form-input-background-focus;\n\t\tborder-color: rgba(82,168,236,.8);\n\t\toutline: 0;\n\t\t.box-shadow(~\"inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6)\");\n\t}\n}\n\n// Fix search field extras in WebKit\n@media screen and (-webkit-min-device-pixel-ratio:0) {\n\tinput[type=\"search\"] {\n\t\tpadding-right: 5px;\n\t\t&::-webkit-search-cancel-button {\n\t\t\t-webkit-appearance: searchfield-cancel-button;\n\t\t}\n\t\t&::-webkit-search-results-button {\n\t\t\t-webkit-appearance: searchfield-results-button;\n\t\t}\n\t\t.box-sizing(border-box);\n\t}\n\tinput[type=\"search\"][results] {\n\t\tpadding-left: 5px;\n\t}\n}\n\n// Add the capslock icon for password fields\ninput[type=\"password\"] {\n\t-webkit-appearance: caps-lock-indicator;\n\t   -moz-appearance: caps-lock-indicator;\n\t    -ms-appearance: caps-lock-indicator;\n\t     -o-appearance: caps-lock-indicator;\n\t        appearance: caps-lock-indicator;\n}\n\n// Position radios and checkboxes better\ninput[type=\"radio\"],\ninput[type=\"checkbox\"] {\n\tmargin: 4px 0 0;\n\tmargin-top: 1px \\9; /* IE8-9 */\n\tline-height: normal;\n}\n\n// Reset width of input images, buttons, radios, checkboxes\ninput[type=\"file\"],\ninput[type=\"image\"],\ninput[type=\"submit\"],\ninput[type=\"reset\"],\ninput[type=\"button\"],\ninput[type=\"radio\"],\ninput[type=\"checkbox\"] {\n\twidth: auto; // Override of generic input selector\n}\n\n// Set the height of select and file controls to match text inputs\nselect,\ninput[type=\"file\"] {\n\t// height: @form-input-height; /* In IE7, the height of the select element cannot be changed by height, only font-size */\n\t// line-height: @form-input-height;\n}\n\n// Make select elements obey height by applying a border\nselect {\n\twidth: 220px; // default input width + 10px of padding that doesn't get applied\n\tborder: 1px solid @form-input-border;\n\tbackground-color: @form-input-background; // Chrome on Linux and Mobile Safari need background-color\n}\n\n// Make multiple select elements height not fixed\nselect[multiple],\nselect[size] {\n\theight: auto;\n}\n\n// Focus for select, file, radio, and checkbox\nselect:focus,\ninput[type=\"file\"]:focus,\ninput[type=\"radio\"]:focus,\ninput[type=\"checkbox\"]:focus {\n\t.tab-focus();\n}\n\n\n//********************************************************\n// UNEDITABLE INPUTS\n\n// Make uneditable inputs look inactive\n.uneditable-input,\n.uneditable-textarea {\n\tcolor: @color-mid-grey;\n\tbackground-color: darken(@form-input-background, 1%);\n\tborder-color: @form-input-border;\n\t.box-shadow(inset 0 1px 2px rgba(0,0,0,.025));\n\tcursor: not-allowed;\n}\n\n// For text that needs to appear as an input but should not be an input\n.uneditable-input {\n\toverflow: hidden; // prevent text from wrapping, but still cut it off like an input does\n\twhite-space: nowrap;\n}\n\n// Make uneditable textareas behave like a textarea\n.uneditable-textarea {\n\twidth: auto;\n\theight: auto;\n}\n\n\n//********************************************************\n// PLACEHOLDER\n\n// Placeholder text gets special styles because when browsers invalidate entire lines if it doesn't understand a selector\ninput,\ntextarea {\n\t.placeholder();\n}\n\n\n//********************************************************\n// CHECKBOXES & RADIOS\n\n// Indent the labels to position radios/checkboxes as hanging\n.radio,\n.checkbox {\n\tmin-height: @type-line-height; // clear the floating input if there is no label text\n\tpadding-left: 20px;\n}\n\n.radio input[type=\"radio\"],\n.checkbox input[type=\"checkbox\"] {\n\tfloat: left;\n\tmargin-left: -20px;\n}\n\n// Move the options list down 