monospace:   Monaco, Menlo, Consolas, \"Courier Prime\", Courier, \"Courier New\", monospace;\n@typeface-small-sans:  \"Lucida Grande\", sans-serif;\n@typeface-base:        @typeface-sans;\n@typeface-alt:         @typeface-serif;\n\n\n//********************************************************\n// TYPE SIZES\n\n@type-size-base:       10px;\n\n@type-size-jumbotron:  @type-size-base * 6.4; // ~64px\n@type-size-biggest:    @type-size-base * 2.8; // ~28px\n@type-size-bigger:     @type-size-base * 2.2; // ~22px\n@type-size-big:        @type-size-base * 1.8; // ~18px\n@type-size-normal:     @type-size-base * 1.4; // ~14px\n@type-size-small:      @type-size-base * 1.2; // ~12px\n\n\n//********************************************************\n// OTHER\n\n@type-line-height-jumbotron: 1em;\n@type-line-height-biggest:   1.2em;\n@type-line-height-bigger:    1.3em;\n@type-line-height-big:       1.4em;\n@type-line-height:           1.5em;\n\n\n//********************************************************\n// MIXINS\n\n#font-weight {\n\t.ultralight() {\n\t\tfont-weight: 100;\n\t}\n\t.light() {\n\t\tfont-weight: 200;\n\t}\n\t.normal() {\n\t\tfont-weight: 400;\n\t}\n\t.semibold() {\n\t\tfont-weight: 500;\n\t}\n\t.bold() {\n\t\tfont-weight: 600;\n\t}\n\t.black() {\n\t\tfont-weight: 900;\n\t}\n}\n\n#font-style {\n\t.shadow() {\n\t\ttext-shadow: 0 1px 0 rgba(0,0,0,.5);\n\t}\n\t.emboss() {\n\t\ttext-shadow: 0 1px 0 rgba(255,255,255,.5);\n\t}\n}\n","//********************************************************\n// Grid system\n\n// Fluid grid\n#grid > .fluid(@fluid-grid-column-width-768, @fluid-grid-gutter-width-768);\n\n// Reset utility classes due to specificity\n[class*=\"span\"].hide,\n.row-fluid [class*=\"span\"].hide {\n\tdisplay: none;\n}\n\n[class*=\"span\"].pull-right,\n.row-fluid [class*=\"span\"].pull-right {\n\tfloat: right;\n}\n\n.row-fluid {\n\tmargin: 0 auto;\n}\n\n// Support max-width switching\n@media (min-width: 1650px) {\n\n\t// body .row-fluid {\n\t// \t.transition(max-width